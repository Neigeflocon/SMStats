create table if not exists `achievements_tf2`
(
	`SteamID` varchar(64) not null default '',
	`ID` int(32) not null default '0',
	`Name` varchar(64) not null default '',
	`Required` int(32) not null default '0',
	`Progress` int(32) not null default '0',
	`Achieved` bool not null default '0',
	primary key (`ID`)
)